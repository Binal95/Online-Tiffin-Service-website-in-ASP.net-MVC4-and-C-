                                    <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown">Report Generation<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="index.html">Customer Report</a>
                                        </li>
                                        <li>
                                            <a href="index2.html">Order Report</a>
                                        </li>
                                        <li>
                                            <a href="index2.html">Driver Report</a>
                                        </li>
                                        <li>
                                            <a href="index2.html">Payment Report</a>
                                        </li>
                                    </ul>
                                </li>*@

                                <li class="dropdown">
                                    <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown">Users<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="@Url.Action("CustomerTable","Admin")">Customer Details</a>
                                        </li>

                                        <li>
                                            <a href="@Url.Action("DriverTable","Admin")">Driver Details</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Area","Admin")">Area We Serve</a>
                                        </li>
                                
                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown">Reviews<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                       
                                        <li>
                                            <a href="@Url.Action("ViewFeedBack","Admin")">View FeedBack</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("ViewComplain","Admin")">View Complain</a>
                                        </li>
                                    </ul>
                                </li>

                                <li class="dropdown">
                                    <a href="@Url.Action("DriverReg","Admin")">Add New Driver</a>
                                </li>

                                @*<li class="dropdown">
        <a href="@Url.Action("AboutUs","Home")" class="dropdown-toggle">About Us <b class="caret"></b></a>
    </li>
    <li class="dropdown">
        <a href="@Url.Action("ContactUs","Home")" class="dropdown-toggle">Contact Us <b class="caret"></b></a>
    </li>
*@

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </header>


        <div class="container">
            @RenderBody()
            @RenderSection("Reg", false)
        </div>


        <footer id="footer">
            <div class="container">
                <div class="col-md-3 col-sm-6">
                    <h4>About us</h4>

                    <p>
                        ChatpataDabba, has created an ecosystem of online meal service for all who stay away from home but wanted to ghar jaisa khana
                        ChatpataDabba! It allows you to order delicious home-style food in convenient packaging- ALL ONLINE.
                    </p>

                    <hr>

                    <hr class="hidden-md hidden-lg hidden-sm">

                </div>

                <!-- /.col-md-3 -->

                <div class="col-md-3 col-sm-6">

                    <h4>Contact</h4>

                    <p>
                        <strong>ChatpataDabba.com</strong>
                        <br />

                        <strong>Baroda</strong>
                    </p>

                    <a href="@Url.Action("ContactUs","Home")" class="btn btn-small btn-template-main">Go to contact page</a>

                    <hr class="hidden-md hidden-lg hidden-sm">

                </div>
            </div>

        </footer>

        <div id="copyright">
            <div class="container">
                <div class="col-md-12">
                    <p class="pull-left">&copy;chatpataDabba.com</p>
                </div>
            </div>
