                                        <li>
                                           <a href="@Url.Action("ViewSampleRequest","Admin")">View Sample Order</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("ViewOrder","Admin")">View Order</a>                                        </li>
                                       
                                        <li>
                                            <a href="@Url.Action("ViewDailyOrder", "Admin")">Code Dispatch</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("CodeRetrival", "Admin")">View Daily Orders</a>
                                        </li>
                                        @*<li>
                                            <a href="index2.html">Pending Orders</a>
                                        </li>
                                        <li>
                                            <a href="index2.html">Order Tracking</a>
                                        </li>*@
