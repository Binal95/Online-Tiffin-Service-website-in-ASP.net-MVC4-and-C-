<body>

    <div id="all">

        <header>

            <div id="top">
                <div class="container">

                    <div class="row">
                        <div class="col-xs-5 contact">
                            <p class="hidden-sm hidden-xs">Contact us on +91 7359857677 or chatapatadabba@gmail.com</p>
                            <p class="hidden-md hidden-lg">
                                <a href="#" data-animate-hover="pulse"><i class="fa fa-phone"></i></a>  <a href="#" data-animate-hover="pulse"><i class="fa fa-envelope"></i></a>
                            </p>
                        </div>

                        <div class="col-xs-7">
                            <div class="login">

                                <ul style="list-style:none ">
                                    <li class="dropdown" id="user">
                                        <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user">Welcome @Session["cname"]<b class="caret"> </b></i></a>
                                        <ul class="dropdown-menu">
                                            <li>
                                                <a href="@Url.Action("Profile", "Customer")" style="color:#555555">Update Profile</a>
                                                @*<a href="contact.html" style="color:#555555">Update Profile</a>*@
                                            </li>
                                            <li>
                                                <a href="@Url.Action("Changepa","Customer")" style="color:#555555">Change Password</a>
                                            </li>

                                            <li>
                                                <a href="@Url.Action("Logout","Customer")" style="color:#555555">Logout</a>
                                            </li>

                                        </ul>
                                    </li>
                                </ul>

                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <div class="navbar-affixed-top" data-spy="affix" data-offset-top="200">

                <div class="navbar navbar-default yamm" role="navigation" id="navbar">

                    <div class="container">
                        <div class="navbar-header">
                            <a class="navbar-brand home" href="@Url.Action("Index","Home")">
                                <img src="@Url.Content("/img/Logo.jpg")" alt="Universal logo" class="hidden-xs hidden-sm">
                            </a>
                            <div class="navbar-buttons">
                                <button type="button" class="navbar-toggle btn-template-main" data-toggle="collapse" data-target="#navigation">
                                    <span class="sr-only">Toggle navigation</span>
                                    <i class="fa fa-align-justify"></i>
                                </button>
                            </div>
                        </div>

                        <div class="navbar-collapse collapse" id="navigation">

                            <ul class="nav navbar-nav navbar-right">

                                <li class="dropdown active">
                                    <a href="@Url.Action("Customer","Customer")" class="dropdown-toggle">Home</a>
                                </li>

                                <li class="dropdown">
                                    <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown">Order<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="@Url.Action("Order","Customer")">PlaceOrder</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("ViewOrderStatus","Customer")">View Orders</a>
                                        </li>

                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown">Daily Order<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="@Url.Action("ViewDailyOrder","Customer")">Today'Order</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("CodeRetrival","Customer")">Today'code</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("ViewDailyOrder","Customer")">Cancle Order</a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a href="@Url.Action("SampleRequest","Customer")" class="dropdown-toggle">Sample tiffin</a>
                                </li>
                                <li class="dropdown">
                                    <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown">Your Views<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li class="dropdown">
                                            <a href="@Url.Action("Feedback","Customer")" style="color:#555555">Provide Feedback</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("Complain","Customer")" style="color:#555555">Complains</a>
                                        </li>

                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a href="javascript: void(0)" class="dropdown-toggle" data-toggle="dropdown">Get Connected<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li class="dropdown">
                                            <a href="@Url.Action("AboutUs", "Customer")" class="dropdown-toggle">About Us </a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("ContactUs", "Customer")" class="dropdown-toggle">Contact Us </a>
                                        </li>

                                    </ul>
                                </li>
                              
                            </ul>

                        </div>
                        <!--/.nav-collapse -->

                        <div class="collapse clearfix" id="search">
                            <form class="navbar-form" role="search">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Search">
                                    <span class="input-group-btn">

                                        <button type="submit" class="btn btn-template-main"><i class="fa fa-search"></i></button>

                                    </span>
                                </div>
                            </form>

                        </div>
                        <!--/.nav-collapse -->

                    </div>


                </div>
                <!-- /#navbar -->

            </div>
        </header>

        <div class="container">
            @RenderBody()
            @RenderSection("Reg", false)
        </div>


        <footer id="footer">
            <div class="container">
                <div class="col-md-3 col-sm-6">
                    <h4>About us</h4>

                    <p>
                        ChatpataDabba, has created an ecosystem of online meal service for all who stay away from home but wanted to ghar jaisa khana
                        ChatpataDabba! It allows you to order delicious home-style food in convenient packaging- ALL ONLINE.
                    </p>

                    <hr>

                    <hr class="hidden-md hidden-lg hidden-sm">

                </div>

                <!-- /.col-md-3 -->

                <div class="col-md-3 col-sm-6">

                    <h4>Contact</h4>

                    <p>
                        <strong>ChatpataDabba.com</strong>
                        <br />

                        <strong>Baroda</strong>
                    </p>

                    <a href="@Url.Action("ContactUs","Home")" class="btn btn-small btn-template-main">Go to contact page</a>

                    <hr class="hidden-md hidden-lg hidden-sm">

                </div>
            </div>

        </footer>
        <div id="copyright">
            <div class="container">
                <div class="col-md-12">
                    <p class="pull-left">&copy;chatpataDabba.com</p>
                </div>
            </div>
        </div>
    </div>
