@{
    ViewBag.Title = "AboutUs";
    Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
}

<section style="background-image:url(@Url.Content("~/img/texture-bw.png")">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="heading">
                    <center> <h2>About Chatpata Dabba</h2></center>
                </div>
