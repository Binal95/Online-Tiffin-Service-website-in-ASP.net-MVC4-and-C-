<link href="~/css/http%20_cdn.datatables.net_plug-ins_3cfcc339e89_integration_bootstrap_3_dataTables.bootstrap.css" rel="stylesheet" />

<div class="row">

    <div class="col-md-12">
        <table id="datatable" class="table table-striped table-bordered" cellspacing="0" width="100">
            <thead>
                <tr>
                    <th>id</th>
                    <th>Customer id</th>
                    
                    <th>Date</th>
                    <th>Complain</th>
                    <th>status</th>
                    <th>DriverID</th>
                </tr>
            </thead>


            <tbody>
                @foreach (var val in Model.ToList())
                {
                    <tr>
                        <td>
                            @val.Id
                        </td>
                        <td>
                            @val.CustId
                        </td>
                        <th>
                            @val.Date
                        </th>
                        <th>
                            @val.Description
                        </th>
                        <th>
                            @val.Status
                        </th>
                        <th>
                            1
                        </th>
                    </tr>
                }




            </tbody>
        </table>
    </div>
</div>
