@model IEnumerable<Project.Models.FeedbackMaster>
@{
    ViewBag.Title = "ViewFeedback";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



<div id="heading-breadcrumbs">
    <div class="container">
        <div class="row">
            <div class="col-md-7">
                <h1>Feedback</h1>
            </div>

        </div>
    </div>
</div>

